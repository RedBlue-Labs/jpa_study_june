<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>tech</servlet-name>
    <servlet-class>tech.listner.TechServlet</servlet-class>
  </servlet>
  
  <listener>
    <listener-class>tech.listner.TechListener</listener-class>
  </listener>
  
  <listener>
    <listener-class>tech.listner.TechRequestListener</listener-class>
  </listener>
  
  <servlet-mapping>
    <servlet-name>tech</servlet-name>
    <url-pattern>/tech</url-pattern>
  </servlet-mapping>
</web-app>
